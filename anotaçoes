build docker file --  docker build -f Dockerfile .

docker build -t app-controle-despesas .   (app-controle-despesas - nome da imagem)

docker run -d --name container_name image_name

complilar aplicação java -- mvn package -Dmaven.test.skip=true  && java -jar target/controle-despesas.jar 

criar um novo container my-python-app-devops-new a partir da imagem my-python-app-devops
mapeia a porta interna 5000 do container para a porta 8080
docker run --name my-python-app-devops-new  -p 8080:5000 my-python-app-devops


